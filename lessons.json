    "theoretical": {
      "t001": {
        "title": "Introducción a los saludos",
        "description": "Los saludos son fundamentales para iniciar cualquier interacción social. En lengua de señas, también tienen su equivalente visual.",
        "imageUrl": "https://firebasestorage.googleapis.com/v0/b/ensenas-cf081.appspot.com/o/theory%2Fintro_saludos.jpg?alt=media&token=abcd1234-abcd-1234-abcd-1234567890ab"
      },
      "t002": {
        "title": "Uso de 'Buenos días'",
        "description": "La seña 'Buenos días' se realiza llevando la mano desde la frente hacia adelante, como saludando con respeto.",
        "imageUrl": "https://firebasestorage.googleapis.com/v0/b/ensenas-cf081.appspot.com/o/theory%2Fbuenos_dias.jpg?alt=media&token=abcd1234-abcd-1234-abcd-1234567890ab"
      },
      "t003": {
        "title": "Seña de 'Gracias'",
        "description": "Para decir 'Gracias', se toca ligeramente el pecho con la palma de la mano abierta y se baja lentamente.",
        "imageUrl": "https://firebasestorage.googleapis.com/v0/b/ensenas-cf081.appspot.com/o/theory%2Fgracias.jpg?alt=media&token=abcd1234-abcd-1234-abcd-1234567890ab"
      },
      "t004": {
        "title": "Cómo decir 'Disculpa'",
        "description": "La seña de 'Disculpa' consiste en hacer un movimiento circular con la mano cerrada cerca del pecho.",
        "imageUrl": "https://firebasestorage.googleapis.com/v0/b/ensenas-cf081.appspot.com/o/theory%2Fdisculpa.jpg?alt=media&token=abcd1234-abcd-1234-abcd-1234567890ab"
      },
      "t005": {
        "title": "Cerrar una conversación",
        "description": "Para indicar que terminaste una conversación, simplemente cierra tu mano como diciendo 'hasta aquí'.",
        "imageUrl": "https://firebasestorage.googleapis.com/v0/b/ensenas-cf081.appspot.com/o/theory%2Fcerrar_conversacion.jpg?alt=media&token=abcd1234-abcd-1234-abcd-1234567890ab"
      }
    }
